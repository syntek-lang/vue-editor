{
  "name": "@syntek/vue-editor",
  "version": "0.0.3",
  "description": "Vue components to easily build Syntek editors",
  "main": "dist/VueEditor.umd.js",
  "module": "src/index.js",
  "unpkg": "dist/VueEditor.umd.min.js",
  "scripts": {
    "test": "npm run lint && npm run build",
    "serve": "vue-cli-service serve --open demo",
    "build": "vue-cli-service build --target lib --name VueEditor ./src/index.js",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext .js,.vue"
  },
  "dependencies": {
    "@syntek/codemirror-mode": "^1.0.1",
    "codemirror": "^5.47.0",
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@syntek/eslint-config-syntek": "^1.1.1",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^7.2.3",
    "eslint-plugin-vue": "^5.2.2",
    "vue-template-compiler": "^2.6.10"
  },
  "publishConfig": {
    "access": "public"
  }
}
